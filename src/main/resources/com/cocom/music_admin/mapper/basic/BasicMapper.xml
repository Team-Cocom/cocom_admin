<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cocom.music_admin.mapper.basic.BasicMapper">
    <insert id="insertGenreName">
        insert into genre_info (gr_name)
        values
        (#{gr_name})
    </insert>
    <select id="selectAllGenreInfos" resultType="com.cocom.music_admin.data.basic.GenreInfo">
        select * from genre_info
    </select>
    <select id="dpchkGenreNames" resultType="java.lang.Integer">
        select count(*) from genre_info where gr_name = #{name}
    </select>
</mapper>